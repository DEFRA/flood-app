{
  "name": "flood-app",
  "version": "8.23.0",
  "description": "Flood risk app",
  "main": "index.js",
  "repository": "github:defra/flood-app",
  "engines": {
    "node": "22.x"
  },
  "browserslist": [
    "IE 11"
  ],
  "scripts": {
    "start": "pm2 start config/pm2.json",
    "start:local": "nodemon --ext js,json,html,env -r dotenv/config index.js",
    "start:local:no-watch": "node -r dotenv/config index.js",
    "build:clean": "build/clean-src",
    "build:cp-assets": "build/cp-assets",
    "build:js": "build/build-js",
    "build:css": "build/build-css",
    "build": "build/build",
    "lint": "standard",
    "unit-test": "export ERRBIT_ENABLED=FALSE && export LOG_LEVEL=silent && FLOOD_APP_LOCAL_CACHE=true && lab --verbose -c -r console -o stdout -r lcov -o coverage/lcov.info --threshold 90 --require dotenv/config",
    "unit-test:no-coverage": "export ERRBIT_ENABLED=FALSE && export LOG_LEVEL=silent && FLOOD_APP_LOCAL_CACHE=true && lab --verbose -r console --require dotenv/config",
    "test": "npm run lint && npm run unit-test",
    "create-release-notes": "node release-docs/lib/create-release-notes.js",
    "postinstall": "npm run build",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') { throw e } }\""
  },
  "author": "defra",
  "license": "ISC",
  "dependencies": {
    "@airbrake/node": "2.1.8",
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.4",
    "@babel/plugin-transform-runtime": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/traverse": "7.28.4",
    "@defra/flood-webchat": "1.0.12",
    "@hapi/boom": "10.0.1",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/code": "9.0.3",
    "@hapi/h2o2": "10.0.4",
    "@hapi/hapi": "21.4.3",
    "@hapi/inert": "7.1.0",
    "@hapi/lab": "26.0.0",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.0.1",
    "@hapi/yar": "11.0.1",
    "@turf/helpers": "6.5.0",
    "@turf/intersect": "6.5.0",
    "@turf/polygon-smooth": "6.5.0",
    "@turf/simplify": "6.5.0",
    "@turf/turf": "6.5.0",
    "@turf/union": "7.1.0",
    "accessible-autocomplete": "2.0.4",
    "assert": "2.1.0",
    "babel-loader": "9.2.1",
    "body-scroll-lock": "3.1.5",
    "core-js": "3.45.1",
    "d3": "7.9.0",
    "datatables.net-buttons": "3.2.5",
    "datatables.net-buttons-dt": "3.2.5",
    "datatables.net-dt": "1.12.1",
    "elm-pep": "1.0.6",
    "geojson": "0.5.0",
    "govuk-frontend": "5.10.0",
    "hapi-pino": "13.0.0",
    "hapi-rate-limit": "8.0.0",
    "joi": "17.13.3",
    "jquery": "3.7.1",
    "lodash.isequal": "4.5.0",
    "micromatch": "4.0.8",
    "moment-timezone": "0.5.48",
    "node-schedule": "2.1.0",
    "nunjucks": "3.2.4",
    "ol": "10.6.0",
    "pino": "9.12.0",
    "pino-abstract-transport": "1.1.0",
    "pino-pretty": "10.2.3",
    "qs": "6.14.1",
    "regenerator-runtime": "0.14.0",
    "sass": "1.93.2",
    "sinon": "18.0.0",
    "standard": "17.1.0",
    "uglify-js": "3.14.4",
    "webpack": "5.102.0",
    "webpack-cli": "6.0.1",
    "yargs": "17.7.2"
  },
  "standard": {
    "ignore": [
      "server/dist",
      "service-down"
    ]
  },
  "devDependencies": {
    "dotenv": "16.6.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "mockdate": "3.0.5",
    "node-html-parser": "6.1.13",
    "nodemon": "3.0.1",
    "proxyquire": "2.1.3",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
