{
  "name": "flood-app",
  "version": "7.1.0",
  "description": "Flood risk app",
  "main": "index.js",
  "repository": "github:defra/flood-app",
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "IE 11"
  ],
  "scripts": {
    "start": "pm2 start config/pm2.json",
    "start:local": "nodemon -r dotenv/config index.js",
    "build:clean": "build/clean-src",
    "build:cp-assets": "build/cp-assets",
    "build:js": "build/build-js",
    "build:css": "build/build-css",
    "build": "build/build",
    "lint": "standard",
    "unit-test": "FLOOD_APP_LOCAL_CACHE=true && lab --verbose -c -r console -o stdout -r lcov -o coverage/lcov.info",
    "test": "npm run lint && npm run unit-test",
    "postinstall": "npm run build",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') { throw e } }\""
  },
  "author": "defra",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@hapi/boom": "^9.1.4",
    "@hapi/catbox-redis": "^6.0.2",
    "@hapi/code": "^8.0.5",
    "@hapi/good": "^9.0.1",
    "@hapi/good-console": "^9.0.1",
    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/h2o2": "^9.1.0",
    "@hapi/hapi": "^20.2.1",
    "@hapi/hoek": "^9.2.1",
    "@hapi/inert": "^6.0.4",
    "@hapi/joi": "^17.1.1",
    "@hapi/lab": "^25.1.3",
    "@hapi/vision": "^6.1.0",
    "@hapi/wreck": "^17.1.0",
    "@hapi/yar": "^10.1.1",
    "@turf/polygon-smooth": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "accessible-autocomplete": "^2.0.4",
    "assert": "^2.0.0",
    "babel-loader": "^9.1.3",
    "body-scroll-lock": "^3.1.5",
    "core-js": "2.6.11",
    "d3": "^7.2.0",
    "datatables.net-buttons": "^2.1.1",
    "datatables.net-buttons-dt": "^2.1.1",
    "datatables.net-dt": "1.12.1",
    "elm-pep": "^1.0.6",
    "geojson": "0.5.0",
    "govuk-frontend": "^3.14.0",
    "hapi-rate-limit": "5.0.1",
    "jquery": "^3.6.0",
    "lodash.isequal": "^4.5.0",
    "moment-timezone": "^0.5.34",
    "node-sass": "^9.0.0",
    "node-schedule": "^2.1.0",
    "nunjucks": "^3.2.4",
    "ol": "^6.9.0",
    "regenerator-runtime": "^0.13.9",
    "sinon": "^12.0.1",
    "standard": "^17.1.0",
    "uglify-js": "^3.14.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "standard": {
    "ignore": [
      "server/dist",
      "service-down"
    ]
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mockdate": "^3.0.5",
    "node-html-parser": "^6.1.1",
    "nodemon": "^3.0.1"
  }
}
